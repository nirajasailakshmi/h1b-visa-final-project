select worksite,year,count(worksite) as cnt,case_status from h1b_final where case_status ='CERTIFIED' and year='2011' group by worksite,year,case_status order by cnt desc limit 5;

select worksite,year,count(worksite) as cyear,case_status from h1b_final where case_status ='CERTIFIED' and year='2012' group by worksite,year,case_status order by cnt desc limit 5;

select worksite,year,count(worksite) as cyear,case_status from h1b_final where case_status ='CERTIFIED' and year='2013' group by worksite,year,case_status order by cnt desc limit 5;

select worksite,year,count(worksite) as cyear,case_status from h1b_final where case_status ='CERTIFIED' and year='2014' group by worksite,year,case_status order by cnt desc limit 5;

select worksite,year,count(worksite) as cyear,case_status from h1b_final where case_status ='CERTIFIED' and year='2015' group by worksite,year,case_status order by cnt desc limit 5;

select worksite,year,count(worksite) as cyear,case_status from h1b_final where case_status ='CERTIFIED' and year='2016' group by worksite,year,case_status order by cnt desc limit 5;
